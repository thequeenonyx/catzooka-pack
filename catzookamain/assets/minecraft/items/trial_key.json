{
  "model": {
    "type": "select",
    "property": "custom_model_data",
      "fallback": {
        "type": "model",
        "model": "item/trial_key"
      },
      "cases": [
        {
          "when": "tpotion",
          "model": {
            "type": "model",
            "model": "item/trophy_potion"
          }
        },
        {
          "when": "tcod",
          "model": {
            "type": "model",
            "model": "item/trophy_cod"
          }
        },
        {
          "when": "tghast",
          "model": {
            "type": "model",
            "model": "item/trophy_ghast"
          }
        },
        {
          "when": "tdisc",
          "model": {
            "type": "model",
            "model": "item/trophy_disc"
          }
        },
        {
          "when": "dark_dollar",
          "model": {
            "type": "model",
            "model": "item/dark_dollar"
          }
        }
      ]
    }
  }